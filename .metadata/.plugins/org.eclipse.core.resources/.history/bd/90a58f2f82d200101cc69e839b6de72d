@SpringBootApplication
@EnableFeignClients(basePackages = "com.jihuarenwu.demo.client")
public class JihuarenwuApplication {

	public static void main(String[] args) {
		SpringApplication.run(JihuarenwuApplication.class, args);
	}

}
